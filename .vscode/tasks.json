{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: clang++ build active file",
      "command": "/usr/bin/clang++",
      "args": [
        "-std=c++11",
        "-g",
        "-I/Users/igessess/Desktop/rphysics/rphysics/include",
        "-L",
        "lib/",
        "-framework",
        "CoreVideo",
        "-framework",
        "IOKit",
        "-framework",
        "Cocoa",
        "-framework",
        "GLUT",
        "-framework",
        "OpenGL",
        "-Wall",
        "lib/libraylib.a",
        "*.cpp",
        "-o",
        "main"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "compiler: /usr/bin/clang++"
    }
  ]
}
